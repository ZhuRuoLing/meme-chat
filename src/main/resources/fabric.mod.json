  "authors": [
    "ZhuRuoLing"
  ],
  "contact": {
    "sources": "https://github.com/ZhuRuoLing/meme-chat"
  },

  "license": "CC0-1.0",
  "icon": "assets/modid/icon.png",

  "environment": "*",
